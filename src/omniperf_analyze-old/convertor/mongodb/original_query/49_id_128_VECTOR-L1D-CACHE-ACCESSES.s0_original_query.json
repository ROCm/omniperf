[
    {"$match": {
    "Index": { "$in": [${DispatchIDFilter:raw}] },
    "gpu-id": { "$in": [${gpuFilter:raw}] },
    "KernelName": { "$in": ${KernelNameFilter:json}}
    }},
    {"$addFields": {
        "denom": {
                "$switch" : {
                "branches": [
                    {
                        "case":  { "$eq": [ $normUnit, "per Wave"]} ,
                        "then":  "&SQ_WAVES"
                    },
                    {
                        "case":  { "$eq": [ $normUnit, "per Cycle"]} ,
                        "then":  "&GRBM_GUI_ACTIVE"
                    },
                    {
                        "case":  { "$eq": [ $normUnit, "per Sec"]} ,
                        "then":  {"$divide":[{"$subtract": ["&EndNs", "&BeginNs" ]}, 1000000000]}
                    }
                ],
                "default": 1
                }         
        } 
    }},
    {"$group": {
        "_id": null,

            "totalReq_avg":{"$avg": { "$divide": [ "&TCP_TOTAL_ACCESSES_sum", "&denom" ] }},
            "totalReq_min":{"$min": { "$divide": [ "&TCP_TOTAL_ACCESSES_sum", "&denom" ] }},
            "totalReq_max":{"$max": { "$divide": [ "&TCP_TOTAL_ACCESSES_sum", "&denom" ] }},

            "readReq_avg":{"$avg": {"$divide": [ "&TCP_TOTAL_READ_sum", "&denom" ] }},
            "readReq_min":{"$min": {"$divide": [ "&TCP_TOTAL_READ_sum", "&denom" ] }},
            "readReq_max":{"$max": {"$divide": [ "&TCP_TOTAL_READ_sum", "&denom" ] }},

            "writeReq_avg":{"$avg": {"$divide": [ "&TCP_TOTAL_WRITE_sum", "&denom" ]}},
            "writeReq_min":{"$min": {"$divide": [ "&TCP_TOTAL_WRITE_sum", "&denom" ]}},
            "writeReq_max":{"$max": {"$divide": [ "&TCP_TOTAL_WRITE_sum", "&denom" ]}},

            "atomicReq_avg":{"$avg": {"$divide": [ { "$add": ["&TCP_TOTAL_ATOMIC_WITH_RET_sum", "&TCP_TOTAL_ATOMIC_WITHOUT_RET_sum"] }, "&denom"]}},
            "atomicReq_min":{"$min": {"$divide": [ { "$add": ["&TCP_TOTAL_ATOMIC_WITH_RET_sum", "&TCP_TOTAL_ATOMIC_WITHOUT_RET_sum"] }, "&denom"]}},
            "atomicReq_max":{"$max": {"$divide": [ { "$add": ["&TCP_TOTAL_ATOMIC_WITH_RET_sum", "&TCP_TOTAL_ATOMIC_WITHOUT_RET_sum"] }, "&denom"]}},

            "cacheBW_avg":{"$avg": { "$divide": [{ "$multiply": ["&TCP_TOTAL_CACHE_ACCESSES_sum", 64 ] }, { "$subtract": ["&EndNs", "&BeginNs"] } ] }},
            "cacheBW_min":{"$min": { "$divide": [{ "$multiply": ["&TCP_TOTAL_CACHE_ACCESSES_sum", 64 ] }, { "$subtract": ["&EndNs", "&BeginNs"] } ] }},
            "cacheBW_max":{"$max": { "$divide": [{ "$multiply": ["&TCP_TOTAL_CACHE_ACCESSES_sum", 64 ] }, { "$subtract": ["&EndNs", "&BeginNs"] } ] }},
            
            "cacheAccess_avg":{"$avg": {"$divide": [ "&TCP_TOTAL_CACHE_ACCESSES_sum", "&denom"]}},
            "cacheAccess_min":{"$min": {"$divide": [ "&TCP_TOTAL_CACHE_ACCESSES_sum", "&denom"]}},
            "cacheAccess_max":{"$max": {"$divide": [ "&TCP_TOTAL_CACHE_ACCESSES_sum", "&denom"]}},

            "cacheHits_avg":{"$avg": {"$divide": [ 
                                { "$subtract": [
                                    "&TCP_TOTAL_CACHE_ACCESSES_sum", 
                                    { "$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"]}
                                ]}, 
                                "&denom"
                        ]}
            },
            "cacheHits_min":{"$min": {"$divide": [ 
                                { "$subtract": [
                                    "&TCP_TOTAL_CACHE_ACCESSES_sum", 
                                    { "$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"]}
                                ]}, 
                                "&denom"
                        ]}
            },
            "cacheHits_max":{"$max": {"$divide": [ 
                                { "$subtract": [
                                    "&TCP_TOTAL_CACHE_ACCESSES_sum", 
                                    { "$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"]}
                                ]}, 
                                "&denom"
                        ]}
            },
            
            "cacheHitRate_avg":{"$avg": {
                "$cond": [{"$ne": ["&TCP_TOTAL_CACHE_ACCESSES_sum", 0]},                 
                            {"$subtract": [100, 
                                            { "$divide": [ 
                                                { "$multiply": [100, 
                                                                { "$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] }
                                                                ] 
                                                }, 
                                                "&TCP_TOTAL_CACHE_ACCESSES_sum"
                                            ]}
                        ]},
                        null
                ]
            }},
            "cacheHitRate_min":{"$min": {
                "$cond": [{"$ne": ["&TCP_TOTAL_CACHE_ACCESSES_sum", 0]},                 
                            {"$subtract": [100, 
                                            { "$divide": [ 
                                                { "$multiply": [100, 
                                                                { "$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] }
                                                                ] 
                                                }, 
                                                "&TCP_TOTAL_CACHE_ACCESSES_sum"
                                            ]}
                        ]},
                        null
                ]
            }},
            "cacheHitRate_max":{"$max": {
                "$cond": [{"$ne": ["&TCP_TOTAL_CACHE_ACCESSES_sum", 0]},                 
                            {"$subtract": [100, 
                                            { "$divide": [ 
                                                { "$multiply": [100, 
                                                                { "$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] }
                                                                ] 
                                                }, 
                                                "&TCP_TOTAL_CACHE_ACCESSES_sum"
                                            ]}
                        ]},
                        null
                ]
            }},
            
            "l2_l1_read_avg":{"$avg": {"$divide": [ "&TCP_TCC_READ_REQ_sum", "&denom"]}},
            "l2_l1_read_min":{"$min": {"$divide": [ "&TCP_TCC_READ_REQ_sum", "&denom"]}},
            "l2_l1_read_max":{"$max": {"$divide": [ "&TCP_TCC_READ_REQ_sum", "&denom"]}},

            "l2_l1_write_avg":{"$avg": {"$divide": [ "&TCP_TCC_WRITE_REQ_sum", "&denom"] }},
            "l2_l1_write_min":{"$min": {"$divide": [ "&TCP_TCC_WRITE_REQ_sum", "&denom"] }},
            "l2_l1_write_max":{"$max": {"$divide": [ "&TCP_TCC_WRITE_REQ_sum", "&denom"] }},


            "l2_l1_atomic_avg":{"$avg": {"$divide": [ { "$add": ["&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] }, "&denom"] }},
            "l2_l1_atomic_min":{"$min": {"$divide": [ { "$add": ["&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] }, "&denom"] }},
            "l2_l1_atomic_max":{"$max": {"$divide": [ { "$add": ["&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] }, "&denom"] }},

            "l2_l1_bw_avg":{"$avg": {"$divide": [{"$multiply": [64, {"$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] } ]}, "&denom" ]}},
            "l2_l1_bw_min":{"$min": {"$divide": [{"$multiply": [64, {"$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] } ]}, "&denom" ]}},
            "l2_l1_bw_max":{"$max": {"$divide": [{"$multiply": [64, {"$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] } ]}, "&denom" ]}},

            "invalidate_avg":{"$avg": {"$divide": [ "&TCP_TOTAL_WRITEBACK_INVALIDATES_sum", "&denom"] }},
            "invalidate_min":{"$min": {"$divide": [ "&TCP_TOTAL_WRITEBACK_INVALIDATES_sum", "&denom"] }},
            "invalidate_max":{"$max": {"$divide": [ "&TCP_TOTAL_WRITEBACK_INVALIDATES_sum", "&denom"] }},


            "l1Latency_avg":{"$avg": {
                "$cond": [
                    {"$ne": ["&TCP_TA_TCP_STATE_READ_sum", 0]},
                    {"$divide": [ "&TCP_TCP_LATENCY_sum", "&TCP_TA_TCP_STATE_READ_sum" ]},
                    null
                ] 
            }},
            "l1Latency_min":{"$min": {
                "$cond": [
                    {"$ne": ["&TCP_TA_TCP_STATE_READ_sum", 0]},
                    {"$divide": [ "&TCP_TCP_LATENCY_sum", "&TCP_TA_TCP_STATE_READ_sum" ]},
                    null
                ] 
            }},
            "l1Latency_max":{"$max": {
                "$cond": [
                    {"$ne": ["&TCP_TA_TCP_STATE_READ_sum", 0]},
                    {"$divide": [ "&TCP_TCP_LATENCY_sum", "&TCP_TA_TCP_STATE_READ_sum" ]},
                    null
                ] 
            }},


            "l2ReadLatency_avg":{"$avg": {
                "$cond": [
                    {"$ne": [{"$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum"]}, 0]},
                    {"$divide": [ "&TCP_TCC_READ_REQ_LATENCY_sum", { "$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum"] }]},
                    null
                ] 
            }},
            "l2ReadLatency_min":{"$min": {
                "$cond": [
                    {"$ne": [{"$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum"]}, 0]},
                    {"$divide": [ "&TCP_TCC_READ_REQ_LATENCY_sum", { "$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum"] }]},
                    null
                ] 
            }},
            "l2ReadLatency_max":{"$max": {
                "$cond": [
                    {"$ne": [{"$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum"]}, 0]},
                    {"$divide": [ "&TCP_TCC_READ_REQ_LATENCY_sum", { "$add": ["&TCP_TCC_READ_REQ_sum", "&TCP_TCC_ATOMIC_WITH_RET_REQ_sum"] }]},
                    null
                ] 
            }},

            "l2WriteLatency_avg":{"$avg": {
                "$cond": [
                    {"$ne": [{"$add": ["&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"]}, 0]},
                    {"$divide": [ "&TCP_TCC_WRITE_REQ_LATENCY_sum", { "$add": ["&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] }]},
                    null
                ]
            }},
            "l2WriteLatency_min":{"$min": {
                "$cond": [
                    {"$ne": [{"$add": ["&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"]}, 0]},
                    {"$divide": [ "&TCP_TCC_WRITE_REQ_LATENCY_sum", { "$add": ["&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] }]},
                    null
                ]
            }},
            "l2WriteLatency_max":{"$max": {
                "$cond": [
                    {"$ne": [{"$add": ["&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"]}, 0]},
                    {"$divide": [ "&TCP_TCC_WRITE_REQ_LATENCY_sum", { "$add": ["&TCP_TCC_WRITE_REQ_sum", "&TCP_TCC_ATOMIC_WITHOUT_RET_REQ_sum"] }]},
                    null
                ]
            }}


    }},
    {"$set": {
    "array": [
        {
            "metric": "Total Req",
            "avg": "&totalReq_avg",
            "min": "&totalReq_min",
            "max": "&totalReq_max",
            "Unit": {"$concat": ["Req ", $normUnit]}
        },
        {
            "metric": "Read Req",
            "avg": "&readReq_avg",
            "min": "&readReq_min",
            "max": "&readReq_max",
            "Unit": {"$concat": ["Req ", $normUnit]}
        },
        {
            "metric": "Write Req",
            "avg": "&writeReq_avg",
            "min": "&writeReq_min",
            "max": "&writeReq_max",
            "Unit": {"$concat": ["Req ", $normUnit]}
        },
        {
            "metric": "Atomic Req",
            "avg": "&l2_l1_atomic_avg",
            "min": "&l2_l1_atomic_min",
            "max": "&l2_l1_atomic_max",
            "Unit": {"$concat": ["Req ", $normUnit]}
        },
        {
            "metric": "Cache BW",
            "avg": "&cacheBW_avg",
            "min": "&cacheBW_min",
            "max": "&cacheBW_max",
            "Unit": "GB/s"
        },
        {
            "metric": "Cache Accesses",
            "avg": "&cacheAccess_avg",
            "min": "&cacheAccess_min",
            "max": "&cacheAccess_max",
            "Unit": {"$concat": ["Req ", $normUnit]}
        },
        {
            "metric": "Cache Hits",
            "avg": "&cacheHits_avg",
            "min": "&cacheHits_min",
            "max": "&cacheHits_max",
            "Unit": {"$concat": ["Req ", $normUnit]}
        },
        {
            "metric": "Cache Hit Rate",
            "avg": "&cacheHitRate_avg",
            "min": "&cacheHitRate_min",
            "max": "&cacheHitRate_max",
            "Unit": "pct"
        },
        {
            "metric": "Invalidate",
            "avg": "&invalidate_avg",
            "min": "&invalidate_min",
            "max": "&invalidate_max",
            "Unit": {"$concat": ["", $normUnit]}
        },
        {
            "metric": "L1-L2 BW",
            "avg": "&l2_l1_bw_avg",
            "min": "&l2_l1_bw_avg",
            "max": "&l2_l1_bw_avg",
            "Unit": {"$concat": ["Bytes ", $normUnit]}
        },
        {
            "metric": "L1-L2 Read",
            "avg": "&l2_l1_read_avg",
            "min": "&l2_l1_read_min",
            "max": "&l2_l1_read_max",
            "Unit": {"$concat": ["Req ", $normUnit]}
        },
        {
            "metric": "L1-L2 Write",
            "avg": "&l2_l1_write_avg",
            "min": "&l2_l1_write_min",
            "max": "&l2_l1_write_max",
            "Unit": {"$concat": ["Req ", $normUnit]}
        },
        {
            "metric": "L1-L2 Atomic",
            "avg": "&l2_l1_atomic_avg",
            "min": "&l2_l1_atomic_min",
            "max": "&l2_l1_atomic_max",
            "Unit": {"$concat": ["Req ", $normUnit]}
        },
        {
            "metric": "L1 Access Latency",
            "avg": "&l1Latency_avg",
            "min": "&l1Latency_min",
            "max": "&l1Latency_max",
            "Unit": "Cycles"
        },
        {
            "metric": "L1-L2 Read Latency",
            "avg": "&l2ReadLatency_avg",
            "min": "&l2ReadLatency_min",
            "max": "&l2ReadLatency_max",
            "Unit": "Cycles"
        },
        {
            "metric": "L1-L2 Write Latency",
            "avg": "&l2WriteLatency_avg",
            "min": "&l2WriteLatency_min",
            "max": "&l2WriteLatency_max",
            "Unit": "Cycles"
        }
        ]
    }},
    {"$unwind": {
    "path": "&array"
    }},
    {"$replaceRoot": {
    "newRoot": "&array"
    }}
]